-- ===============================
-- safety_timers
-- ===============================
CREATE TABLE safety_timers (
    timer_id INT PRIMARY KEY,
    user_id INT,
    expiry_time DATETIME,
    is_active BOOLEAN,
    start_location_id INT,
    end_location_id INT
);

