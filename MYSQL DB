-- ===============================
-- offline_reports
-- ===============================
CREATE TABLE offline_reports (
    id INT PRIMARY KEY,
    user_id INT,
    name VARCHAR(50),
    contact BIGINT,
    category VARCHAR(50),
    description TEXT,
    location_id INT,
    time DATETIME
);

