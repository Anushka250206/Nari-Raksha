
-- NARIRAKSHA Complete SQL File
-- ===============================

DROP DATABASE IF EXISTS NARIRAKSHA;
CREATE DATABASE NARIRAKSHA;
USE NARIRAKSHA;

-- ===============================
-- admin
-- ===============================
CREATE TABLE admin (
    id INT PRIMARY KEY,
    adname VARCHAR(50),
    password VARCHAR(50)
);

INSERT INTO admin VALUES
(1, 'admin', 'Anushroot');

-- ===============================
-- users
-- ===============================
CREATE TABLE users (
    user_id INT PRIMARY KEY,
    name VARCHAR(50),
    contact BIGINT,
    email VARCHAR(100),
    password VARCHAR(50)
);

